{
	"name": "bwomp",
	"description": "teeth-included petite framework for fastify",
	"author": "Faye Keller (pinkcig)",
	"version": "1.0.0",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"format": "prettier --write '**/*.{ts,yml,json,md}' --ignore-path .gitignore"
	},
	"dependencies": {
		"@pinkcig/console": "^1.0.3",
		"@sapphire/shapeshift": "^3.8.0",
		"fastify": "^4.10.2"
	},
	"devDependencies": {
		"@pinkcig/config": "^1.0.5",
		"@sapphire/ts-config": "^3.3.4",
		"prettier": "^2.8.1",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.2.4",
	"prettier": "@pinkcig/config"
}
